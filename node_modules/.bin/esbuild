#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/shreyas/cc-5/cc5-ds-algorithms/Backend/Library-management-system/node_modules/esbuild/bin/node_modules:/home/shreyas/cc-5/cc5-ds-algorithms/Backend/Library-management-system/node_modules/esbuild/node_modules:/home/shreyas/cc-5/cc5-ds-algorithms/Backend/Library-management-system/node_modules:/home/shreyas/cc-5/cc5-ds-algorithms/Backend/node_modules:/home/shreyas/cc-5/cc5-ds-algorithms/node_modules:/home/shreyas/cc-5/node_modules:/home/shreyas/node_modules:/home/node_modules:/node_modules:/home/shreyas/cc-5/cc5-ds-algorithms/Backend/Library-management-system/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/shreyas/cc-5/cc5-ds-algorithms/Backend/Library-management-system/node_modules/esbuild/bin/node_modules:/home/shreyas/cc-5/cc5-ds-algorithms/Backend/Library-management-system/node_modules/esbuild/node_modules:/home/shreyas/cc-5/cc5-ds-algorithms/Backend/Library-management-system/node_modules:/home/shreyas/cc-5/cc5-ds-algorithms/Backend/node_modules:/home/shreyas/cc-5/cc5-ds-algorithms/node_modules:/home/shreyas/cc-5/node_modules:/home/shreyas/node_modules:/home/node_modules:/node_modules:/home/shreyas/cc-5/cc5-ds-algorithms/Backend/Library-management-system/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../esbuild/bin/esbuild"   "$@"
exit $?
